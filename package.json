{
  "name": "reactexpress",
  "version": "1.0.0",
  "description": "Boilerplate for React and express",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "server": "nodemon server.js",
    "react-ad-watcher": "cd react-ad-watcher && npm start",
    "dev": "concurrently \"npm run server\" \"npm run react-ad-watcher\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix react-ad-watcher && npm run build --prefix react-ad-watcher"
  },
  "author": "Umar Farooq",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "concurrently": "^5.3.0",
    "connect-mongo": "^3.2.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.2.0",
    "express-fileuploader": "0.0.7",
    "express-session": "^1.17.1",
    "is-empty": "^1.2.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.11.8",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "validator": "^13.5.2"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "gh-pages": "^3.1.0",
    "nodemon": "^2.0.6"
  }
}
